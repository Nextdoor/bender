{
  "handler": {
    "config_name": "S3Handler",
    "fail_on_exception": true
  },
  "sources": [
    {
      "name": "Logs",
      "source_regex": ".*",
      "deserializer": {
        "config_name": "GenericJson"
      },
      "mutators": [
        {
          "config_name": "KeyNameMutator"
        },
        {
          "config_name": "DropArraysMutator"
        }
      ]
    }
  ],
  "wrapper": {
    "config_name": "PassthroughWrapper"
  },
  "serializer": {
    "config_name": "Json"
  },
  "transport": {
    "config_name": "File",
    "threads": 1,
    "filename": "/tmp/fooq.json"
  },
  "reporters": [
    {
      "config_name": "DataDog"
    }
  ]
}