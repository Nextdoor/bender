{
  "handler": {
    "config_name": "DummyHandlerHelper$DummyHandler"
  },
  "sources": [
    {
      "name": "Test Events",
      "source_regex": ".*",
      "deserializer": {
        "config_name": "GenericJson",
        "nested_field_configs": [
          {
            "field": "MESSAGE",
            "prefix_field": "MESSAGE_PREFIX"
          }
        ]
      },
      "operations": [
        {
          "config_name": "TimeOperation",
          "time_field": "$.EPOCH",
          "time_field_type": "SECONDS"
        },
        {
          "config_name": "JsonDropArraysOperation"
        },
        {
          "config_name": "JsonKeyNameOperation"
        }
      ]
    }
  ],
  "wrapper": {
    "config_name": "KinesisWrapper"
  },
  "serializer": {
    "config_name": "Json"
  },
  "transport": {
    "config_name": "File",
    "threads": 1,
    "filename": "/tmp/bender.out"
  },
  "reporters": [
    {
      "config_name": "DataDog"
    }
  ]
}